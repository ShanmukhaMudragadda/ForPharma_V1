  "main": "main.ts",
  "type": "module",
  "scripts": {
    "start": "tsx main.ts",
    "dev": "tsx watch main.ts",
    "dev:nodemon": "nodemon",
    "build": "tsc",
    "prisma:generate": "npm run prisma:generate:shared && npm run prisma:generate:tenant",
    "prisma:generate:shared": "prisma generate --schema=./prisma/schema.shared.prisma",
    "prisma:generate:tenant": "prisma generate --schema=./prisma/schema.tenant.prisma",
    "prisma:migrate:shared": "prisma migrate dev --schema=./prisma/schema.shared.prisma",
    "prisma:migrate:tenant": "prisma migrate dev --schema=./prisma/schema.tenant.prisma",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
